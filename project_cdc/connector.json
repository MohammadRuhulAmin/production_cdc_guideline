{
    "name": "cdc-connector", 
    "config": {
      "connector.class": "io.debezium.connector.mysql.MySqlConnector",
      "tasks.max": "1",
      "database.hostname": "", 
      "database.port": "3306",
      "database.user": "",
      "database.password": "",
      "database.server.id": "1",  
